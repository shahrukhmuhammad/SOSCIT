                            else if (item.DenominationTitle == '50') {
                                $("#numberOfPacketsFive").val(item.NumberOfBundles);
                                $("#totalValueFive").val(item.TotalValue);
                                $("#SealNoFive").val(item.SealNo);


                                $("#fiftyDenomBundle").text(item.NumberOfBundles);
                                $("#fiftyDenomAmount").text(item.TotalValue);
                                $("#fiftyDenomSeal").text(item.SealNo);

                                $("#inpfiftyDenomBundle0").val(item.NumberOfBundles);
                                $("#inpfiftyDenomAmount0").val(item.TotalValue);
                                loadEmployees('inpfiftySorter0');
                                $("#50").show();

                            }
                            else if (item.DenominationTitle == '100') {
                                $("#numberOfPacketsSix").val(item.NumberOfBundles);
                                $("#totalValueSix").val(item.TotalValue);
                                $("#SealNoSix").val(item.SealNo);

                                $("#hundredDenomBundle").text(item.NumberOfBundles);
                                $("#hundredDenomAmount").text(item.TotalValue);
                                $("#hundredDenomSeal").text(item.SealNo);

                                $("#inphundredDenomBundle0").val(item.NumberOfBundles);
                                $("#inphundredDenomAmount0").val(item.TotalValue);
