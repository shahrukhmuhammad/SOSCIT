                                            @Html.TextBoxFor(x => x.OrderNumber, new { @class = "form-control input-sm", @required = "required", @readonly = "readonly" })
                                            @Html.ValidationMessageFor(m => m.OrderNumber)
                                            @Html.HiddenFor(x => x.OrderBookingId)
