        function AddNewRow(panelId, rowCounter, panelIndexer, rowName, denom) {
            debugger
            var denomTitle = denomNumberToTitle(denom);
            rowCounter = parseInt(rowCounter) + 1;
            if (rowCounter > 1)
                $('#' + panelId).parent().children().last().after(generateNewRowHtml(rowCounter, panelIndexer, rowName, denom, denomTitle));
            else
                $('#' + panelId).after(generateNewRowHtml(rowCounter, panelIndexer, rowName, denom, denomTitle));

            var counterTemp = parseInt(rowCounter) - parseInt(1);
            loadEmployees('inp' + denomTitle +'Sorter' + counterTemp);
            $(".select2").select2();
        }

        function generateNewRowHtml(rowCounter, panelIndexer, rowName, denom, denomTitle) {
            //debugger
            var counterTemp = parseInt(rowCounter) - parseInt(1);
            //alert(counterTemp);
            var _html = '<div class="row" id="' + rowName+'' + rowCounter + '" data-id="' + rowCounter + '"><div class="col-sm-3"><div class="row form-group">'
            _html += '<label class="col-sm-3 text-left horizontal-label">Sorter</label><div class="col-sm-9">'
            _html += '<select id="inp' + denomTitle +'Sorter' + counterTemp + '" name="CPCVaultConsolidatedBundle[' + panelIndexer + '].CPCVaultConsolidatedBundlesDetails[' + counterTemp + '].SorterId" class="form-control input-sm select2 clearField"><option> - Select - </option></select></div></div></div >'
            _html += '<div class="col-sm-3"><div class="row form-group"><label class="col-sm-6 text-left horizontal-label">Volume in Bundles</label><div class="col-sm-6">'
            _html += '<input id="inp' + denomTitle +'DenomBundle' + rowCounter + '" data-denom="' + denom +'" type="number" class="form-control input-sm inp totalfinalNumber" name="CPCVaultConsolidatedBundle[' + panelIndexer + '].CPCVaultConsolidatedBundlesDetails[' + counterTemp + '].NumberOfBundles" required="required" /></div></div></div><div class="col-sm-4">'
            _html += '<div class="row form-group"><label class="col-sm-6 text-left horizontal-label">Assigned Amount</label><div class="col-sm-6">'
            _html += '<input id="inp' + denomTitle +'DenomAmount' + rowCounter + '" type="number" class="form-control input-sm" name="CPCVaultConsolidatedBundle[' + panelIndexer + '].CPCVaultConsolidatedBundlesDetails[' + counterTemp + '].Amount" readonly="readonly" /></div></div></div>'
            _html += '<div class="col-sm-2"><a href="javascript://" class="btn btn-xs btn-danger btn-removeRow" data-original-title="Delete"><i class="fa fa-trash"></i> Delete</a></div></div>'
            return _html;
        }

        function denomNumberToTitle(denomNumber) {
            var denomTitle = '';
            switch (parseInt(denomNumber)) {
                case 10:
                    denomTitle = "ten";
                    break;
                case 20:
                    denomTitle = "twnety";
                    break;
                case 50:
                    denomTitle = "fifty";
                    break;
                case 100:
                    denomTitle = "hundred";
                    break;
                case 500:
                    denomTitle = "fiveHundred";
                    break;
                case 1000:
                    denomTitle = "thousand";
                    break;
                case 5000:
                    denomTitle = "fiveThousand";
            }
            return denomTitle;
        }

        $(document).ready(function () {



            $('.btn-custom').click(function (e) {
                debugger
                var denom = $(this).attr("data-denom");
                var rowName = $(this).attr("data-id");
                var divId = $(this).parent().parent().attr("id");
                var rowCounter = $(this).parent().parent().parent().children().last().attr("data-id"); //Row Counter
                var panelIndexer = $(this).parent().parent().parent().attr("data-id"); //Row Counter
                AddNewRow(divId, rowCounter, panelIndexer, rowName, denom);
            });


            var employeeArray = @Html.Raw(Json.Encode(ViewBag.EmployeeList));
            generateEmployeeDDL(employeeArray);

            function generateEmployeeDDL(array) {
                //debugger
                for (var i = 0; i < array.length; i++) {
                    $("#listEmployee").append($("<option /> ").val(array[i].Value).text(array[i].Text));
                }
            }

            //Add Shipping Company
            $('.addNewCompany').on('click', function () {
                $('#loading-mask').show();
                var target = "";
                target = "/CPC/VaultCustodian/_VaultCustodianList";
                // load the url and show modal on success
                $("#addNewCompany-Modal .modal-dialog").load(target, function () {
                    $("#addNewCompany-Modal").modal("show");
                    filterDataTable("#VaultCustodian-dataTable", [0, 1, 2, 3, 4], [5]);
                    $('#loading-mask').hide();
                });
            });
            //"#DateOfCashReturn"
            $("#Date").data("DateTimePicker").date(moment('@Model.Date.ToShortDateString()'));

            $(".select2").select2();

            //input fields leave event
            $(document).on('focusout', '.inp', function (e) {
                e.preventDefault();
                //debugger;
                if ($(this).val()) {

                    var inpId = $(this).attr("id");
                    denomTitle = $(this).attr("data-denom");
                    const totalValue = GetTotalVal($(this).val(), denomTitle);
                    $(this).parent().parent().parent().next('div').find('input').val(totalValue);

                    var divMasterId = $(this).parent().parent().parent().parent().parent().attr("id");
                    var checktotal = CheckDenomTotal(denomTitle, divMasterId);
                    if (!checktotal) {
                        //alert("Assigned Bundles should be less than or equal to total bundles");
                        //bootbox.confirm("Assigned Bundles should be less than or equal to total bundles");
                        bootbox.dialog({
                            message: "Assigned Bundles should be less than or equal to total bundles",
                            title: "Value Exceeded",
                            buttons: {
                                main: {
                                    label: "Ok",
                                    className: "btn-danger",
                                    callback: function () {
                                        show: false;
                                        $('#' + inpId).val('');
                                        //alert($('#' + inpId).val(''));
                                        //console.log($('#' + inpId))
                                    }
                                },
                            }

                        });
                    }
                    ///console.log($('#numberOfPacketsThree').val());
                }
                //$('#' + inpId).focus();
            });
        });

        function CheckDenomTotal(denomTitle, divMasterId) {
            var totalBundles = 0;
            var assignedBundles = 0;
            //debugger;
            if (denomTitle == '10') {
                totalBundles = $('#numberOfPacketsThree').val();
                return compareTotalDemonNo(divMasterId, assignedBundles, totalBundles);
            }
            else if (denomTitle == '20') {
                totalBundles = $('#numberOfPacketsFour').val();
                return compareTotalDemonNo(divMasterId, assignedBundles, totalBundles);
            }
            else if (denomTitle == '50') {
                totalBundles = $('#numberOfPacketsFive').val();
                return compareTotalDemonNo(divMasterId, assignedBundles, totalBundles);
            }
            else if (denomTitle == '100') {
                totalBundles = $('#numberOfPacketsSix').val();
                return compareTotalDemonNo(divMasterId, assignedBundles, totalBundles);
            }
            else if (denomTitle == '500') {
                totalBundles = $('#numberOfPacketsSeven').val();
                return compareTotalDemonNo(divMasterId, assignedBundles, totalBundles);
            }
            else if (denomTitle == '1000') {
                totalBundles = $('#numberOfPacketsEight').val();
                return compareTotalDemonNo(divMasterId, assignedBundles, totalBundles);
            }
            else if (denomTitle == '5000') {
                totalBundles = $('#numberOfPacketsNine').val();
                return compareTotalDemonNo(divMasterId, assignedBundles, totalBundles);
            }
        }

        function compareTotalDemonNo(divMasterId, assignedBundles, totalBundles) {
            $('#' + divMasterId + ' :input.totalfinalNumber').each(function () {
                assignedBundles += parseInt($(this).val());
            });
            if (assignedBundles > totalBundles)
                return false;
            else
                return true;
        }

        function GetTotalVal(unsortedValue, denomVal) {
            const totalVal = parseInt(unsortedValue) * parseInt(denomVal) * 1000;
            return isNaN(totalVal) ? '0' : totalVal;
        }

